{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "agent_num":{
        "type": "integer",
        "minimum": 1
      },
      "obs_dim": {
        "type": "integer",
        "minimum": 1
      },
      "act_dim": {
        "type": "integer",
        "minimum": 1
      },
      "hidden_layers_actor": {
        "type": "array",
        "items": {
          "type": "integer",
          "minimum": 1
        },
        "minItems": 1
      },
      "hidden_layers_critic": {
        "type": "array",
        "items": {
          "type": "integer",
          "minimum": 1
        },
        "minItems": 1
      },
      "lr_actor": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "lr_critic": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "gamma": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "replay_size": {
        "type": "integer",
        "minimum": 1
      },
      "batch_size": {
        "type": "integer",
        "minimum": 1
      },
      "noise_type":{
        "type": "string"
      },
      "noise_sigma":{
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "noise_max": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "noise_min": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "noise_decay": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "update_after": {
        "type": "integer",
        "minimum": 0
      },
      "update_online_every": {
        "type": "integer",
        "minimum": 1
      },
      "seed": {
        "type": [
          "integer",
          "null"
        ]
      },
      "dtype":{
        "type": "string"
      }
    },
    "required": [
      "obs_dim",
      "act_dim"
    ]
  }
  